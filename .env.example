# Supabase
NEXT_PUBLIC_SUPABASE_URL=your-project-url.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-key
STRIPE_SECRET_KEY=sk_test_your-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Stripe Price IDs (create these in Stripe Dashboard)
# Starter Pack: $20 for 4 credits
STRIPE_PRICE_ID_STARTER=price_starter_xxx

# Pro Pack: $80 for 20 credits
STRIPE_PRICE_ID_PRO=price_pro_xxx

# Corporate Pack: $175 for 50 credits
STRIPE_PRICE_ID_CORPORATE=price_corporate_xxx

# Note: Enterprise is contact-only, no Stripe price ID needed

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Song Generation API (your existing integration)
SONG_GENERATION_API_KEY=your-api-key
SONG_GENERATION_API_URL=https://your-song-api.com/generate
